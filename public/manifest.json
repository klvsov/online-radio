{
  "manifest_version": 3,
  "name": "Online Radio",
  "version": "1.0",
  "description": "Free radio online!",
  "icons": {
    "16": "radio.svg",
    "48": "radio.svg",
    "128": "radio.svg"
  },
  "action": {
    "default_action": "radio.svg",
    "default_title": "Online Radio",
    "default_popup": "index.html"
  },
  "permissions": ["storage", "notifications", "alarms"]
}
